{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        //"es6": true
    },
    "extends": ["eslint:recommended" , "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars" : "warn", //사용하지 않는 변수선언
        "no-console" : "off" //콘솔미사용설정
    }
}
